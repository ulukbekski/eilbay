{
  "compilerOptions": {
    "baseUrl":".",

    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": ["./*"],
      "@elements*": ["components/elements/*"],
      "@UI/*":["components/UI/*"],
      "@Layout/*":["components/Layout/*"],
      "@assets/*": ["assets/*"],
      "@icons/*": ["assets/icons/*"],
      "@styles/*": ["styles/*"],
      "@store/*": ["store/*"],
      "@hooks/*": ["hooks/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
